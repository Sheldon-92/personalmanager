#!/bin/bash
# Calendar专用命令 - 自动切换到Calendar账号

# 切换到Calendar账号
python3 ~/programs/personal-manager/scripts/multi_account_auth.py switch calendar 2>/dev/null

# 执行Calendar相关命令
if [ $# -eq 0 ]; then
    # 如果没有参数，显示Calendar同步
    ./bin/pm-local calendar sync
else
    # 如果有参数，传递给calendar命令
    ./bin/pm-local calendar "$@"
fi